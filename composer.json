{
  "name": "spiral/cycle",
  "type": "library",
  "description": "DataMapper and ORM for long-running processes",
  "require": {
    "php": "^7.1",
    "ext-pdo": "*",
    "spiral/core": "^1.1",
    "spiral/database": "^2.1",
    "spiral/tokenizer": "^2.1",
    "doctrine/inflector": "^1.3",
    "doctrine/collections": "^1.5",
    "zendframework/zend-hydrator": "^2.4",
    "ocramius/proxy-manager": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "^1.1",
    "spiral/debug": "^1.3",
    "ramsey/uuid": "^3.8"
  },
  "autoload": {
    "psr-4": {
      "Spiral\\Cycle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Spiral\\Cycle\\Tests\\": "tests/Cycle/"
    }
  }
}
